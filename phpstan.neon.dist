includes:
    - dev/tools/StaticAnalyze/PHPStan/extension.neon

parameters:
    level: max
    tmpDir: ./build/cache/phpstan
    tipsOfTheDay: false
    treatPhpDocTypesAsCertain: false
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    paths:
        - app/src/
        - lib/internal/
        - dev/tests/
